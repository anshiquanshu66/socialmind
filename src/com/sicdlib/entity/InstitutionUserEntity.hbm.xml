<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sicdlib.entity.InstitutionUserEntity" table="institution_user" schema="socialmind">
        <id name="userId" column="userID"/>
        <property name="bossName" column="bossName"/>
        <property name="idFrontUrl" column="IDFrontUrl"/>
        <property name="idBackUrl" column="IDBackUrl"/>
        <property name="institutionName" column="institutionName"/>
        <property name="type" column="type"/>
        <property name="institudeCode" column="institudeCode"/>
        <property name="institudeCodeUrl" column="institudeCodeUrl"/>
    </class>
</hibernate-mapping>