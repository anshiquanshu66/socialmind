<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.InstitutionUserEntity" table="institution_user" schema="socialmind">
        <id name="userId" column="userID"/>
        <property name="bossName" column="bossName"/>
        <property name="idFront" column="IDFront"/>
        <property name="idBack" column="IDBack"/>
        <property name="institutionName" column="institutionName"/>
        <property name="type" column="type"/>
        <property name="institudeCode" column="institudeCode"/>
        <property name="registerGraph" column="registerGraph"/>
    </class>
</hibernate-mapping>