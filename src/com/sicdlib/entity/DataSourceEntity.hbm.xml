<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sicdlib.entity.DataSourceEntity" table="data_source" schema="socialmind">
        <id name="tableNameId" column="tableNameID"/>
        <property name="tableName" column="tableName"/>
        <property name="count" column="count"/>
        <many-to-one name="websiteEntity" column="websiteID"></many-to-one>
    </class>
</hibernate-mapping>