<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.ToolkitEntity" table="toolkit" schema="socialmind">
        <id name="toolkitId" column="toolkitID"/>
        <property name="toolkitName" column="toolkitName"/>
        <property name="toolkitDescribe" column="toolkitDescribe"/>
        <property name="userId" column="userID"/>
    </class>
</hibernate-mapping>