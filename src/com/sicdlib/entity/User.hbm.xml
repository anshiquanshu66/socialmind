<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sicdlib.entity">
	<class name="User" table="tb_user">
		<id name="u_id">
			<generator class="native" />
		</id>
		<property name="u_name" length="50" />
		<property name="u_pwd" length="128" />
		<property name="u_telphone" length="30" />
		<property name="u_mail" length="30" />
		<property name="u_sex" type="boolean"/>
	</class>
</hibernate-mapping>

