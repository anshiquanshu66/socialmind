<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sicdlib.entity.NoticeEntity" table="notice" schema="socialmind">
        <id name="noticeId" column="noticeID"/>
        <property name="noticeTitle" column="noticeTitle"/>
        <property name="noticeContent" column="noticeContent"/>
        <property name="noticTime" column="noticTime"/>
        <property name="adminId" column="adminID"/>
    </class>
</hibernate-mapping>