<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.BulletinEntity" table="bulletin" schema="socialmind">
        <id name="bulletinId" column="bulletinID"/>
        <property name="name" column="name"/>
        <property name="introduction" column="introduction"/>
        <property name="bulletinUrl" column="bulletinURL"/>
        <property name="bulletinType" column="bulletinType"/>
        <property name="generateTime" column="generateTime"/>
        <property name="bulletinStyleId" column="bulletinStyleID"/>
    </class>
</hibernate-mapping>