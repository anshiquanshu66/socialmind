<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.SpiderErrorLogEntity" table="spider_error_log" schema="socialmind">
        <id name="logId" column="logID"/>
        <property name="ip" column="IP"/>
        <property name="errorItem" column="errorItem"/>
        <property name="errorDetails" column="errorDetails"/>
        <property name="websiteId" column="websiteID"/>
        <property name="spiderId" column="spiderID"/>
        <property name="errorTime" column="errorTime"/>
    </class>
</hibernate-mapping>