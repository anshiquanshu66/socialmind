<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.SpiderConfigItemEntity" table="spider_config_item" schema="socialmind">
        <id name="spiderConfigItemId" column="spiderConfigItemID"/>
        <property name="spiderConfigId" column="spiderConfigID"/>
        <property name="configItemName" column="configItemName"/>
        <property name="configItemValue" column="configItemValue"/>
        <property name="note" column="note"/>
    </class>
</hibernate-mapping>