<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sicdlib.entity.MessageEntity" table="message" schema="socialmind">
        <id name="messageId" column="messageID"/>
        <property name="messageTitle" column="messageTitle"/>
        <property name="messageContent" column="messageContent"/>
        <property name="messageTime" column="messageTime"/>
        <property name="adminId" column="adminID"/>
    </class>
</hibernate-mapping>