<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.CompanyUserEntity" table="company_user" schema="socialmind">
        <id name="userId" column="userID"/>
        <property name="bossName" column="bossName"/>
        <property name="idFront" column="IDFront"/>
        <property name="companyName" column="companyName"/>
        <property name="type" column="type"/>
        <property name="idBack" column="IDBack"/>
        <property name="businessLicence" column="businessLicence"/>
    </class>
</hibernate-mapping>