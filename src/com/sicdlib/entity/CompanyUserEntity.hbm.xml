<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.CompanyUserEntity" table="company_user" schema="socialmind">
        <id name="userId" column="userID"/>
        <property name="bossName" column="bossName"/>
        <property name="idFrontUrl" column="IDFrontUrl"/>
        <property name="companyName" column="companyName"/>
        <property name="type" column="type"/>
        <property name="idBackUrl" column="IDBackUrl"/>
        <property name="businessLicenceUrl" column="businessLicenceUrl"/>
        <property name="businessLicenceId" column="businessLicenceId"/>
    </class>
</hibernate-mapping>