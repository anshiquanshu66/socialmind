<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sicdlib.entity.EventEntity" table="event" schema="socialmind">
        <id name="objectId" column="objectID"/>
        <property name="eventSummary" column="eventSummary"/>
        <property name="eventBeainTime" column="eventBeainTime"/>
        <property name="eventOverTime" column="eventOverTime"/>
        <property name="introduction" column="introduction"/>
    </class>
</hibernate-mapping>